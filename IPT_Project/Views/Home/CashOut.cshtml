
@{
    ViewBag.Title = "CashOut";
}



<html>
<head>
    <meta charset="UTF-8">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js">
    </script>
    <script src="~/Scripts/script/jquery-3.6.0.js"></script>
    <script src="~/Scripts/script/jquery-3.6.0.min.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link href="~/Content/downloaded_css/w3.css" rel="stylesheet" />
    
    <link href="~/Content/User_Account_Css/CashOutStyle.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.2/css/all.css">
</head>
<body>
   
    <div class="signup-form">
        <form>
            <h2 class="header_cashin"><b>Cash-Out Doorway</b></h2>
            <p class="hint-txt">Provide your information below!</p>

            <div class="form-group">

                <div class="row">
                    <div class="col-xs-6"><input type="text" id="txtbxci_ownername" class="txtbxfn" placeholder="Owner Name" required="required"></div>

                </div>

                <div class="col-xs-6"><input type="text" id="txtbxci_cardnumber" data-mask="0000 0000 0000 0000" class="txtbxln" placeholder="Card Number"></div>
            </div>

            <div class="form-group">
                <input type="text" id="txtbxci_amount" class="txtbxamount" placeholder="Amount" required="required">
                <input type="text" id="txtbxci_contactnum" class="txtbxcontactnumber" data-mask="0000 000 0000 " placeholder="Contact Number(+63)" required="required">
            </div>


            <div class="contact">
                
                <br />
                <br />
            </div>


            <div class="form-group">
                <label class="checkbox-inline"><input type="checkbox" required="required"> I accept the <a href="#" style="color:white">Terms of Use</a> &amp; <a href="#" style="color:white">Privacy Policy</a></label>
            </div>


            <div class="form-group">
                <button type="submit" id="btnCash_out" class="btn btn-success btn-lg btn-block"><b>Cash Out</b></button>
            </div>

        </form>

    </div>


</body>

<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.15/jquery.mask.min.js"></script>
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>


    <script src="~/Scripts/MyScript/Cashout.js"></script>
</html>



