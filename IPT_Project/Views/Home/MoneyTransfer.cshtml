
@{
    ViewBag.Title = "MoneyTransfer";
}





<html>
<head>
    <meta charset="UTF-8">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js">
    </script>
    <script src="~/Scripts/script/jquery-3.6.0.js"></script>
    <script src="~/Scripts/script/jquery-3.6.0.min.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link href="~/Content/downloaded_css/w3.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.2/css/all.css">
    <link href="~/Content/User_Account_Css/MoneyTransferStyle.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.2/css/all.css">
</head>
<body>
    

    <div class="signup-form">
        <form>
            <h2 class="header_cashin"><b>Money Transfer</b></h2>
            <p class="hint-txt">Send your money to a friend!</p>

            <div class="firstrow">
                <label class="senderinfo">Sender Information</label>
                <div>
                    <div class="col-xs-6"><input type="text" id="txtbxsender_ownername" class="txtbxsenderinfo_name" placeholder="Sender's Name" required="required"></div>
                    </br>
                    </br>
                </div>

                <div>
                    <div class="col-xs-6"><input type="text" id="txtbxsender_contactnum" class="txtbxsenderinfo_contactnumber"  data-mask="0000 000 0000 " placeholder="Contact Number (+63)" required="required"></div>
                    </br>
                    </br>
                </div>

                <div>
                    <div class="col-xs-6"><input type="text" id="txtbxsender_amount" class="txtbxsenderinfo_amount" placeholder="Amount" required="required"></div>

                </div>


                <div class="secondrow">
                    <label class="receiverinfo">Receiver Information</label>
                    </br>
                    </br>

                </div>

                <div>
                    <div class="col-xs-6"><input type="text" id="txtbxreceiver_name" class="txtbxsenderinfo_amount" placeholder="Receiver's Name" required="required"></div>

                </div>

                <div>
                    <div class="col-xs-6"><input type="text" id="txtbxreceiver_contactname" class="txtbxreceiverinfo_contactnumber"  data-mask="0000 000 0000 " placeholder="Contact Number (+63)" required="required"></div>
                    </br>
                    </br>
                </div>

                <div class="form-group">
                    <button type="submit" id="btnMoneyTransfer_Submit" class="btn btn-success btn-lg btn-block"><b>Submit</b></button>
                </div>

            </div>
 
        </form>

    </div>


</body>

<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.15/jquery.mask.min.js"></script>
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>

<script src="~/Scripts/MyScript/MoneyTransfer.js"></script>
</html>




